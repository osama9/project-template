<body>
<partial name="_Header"/>
<div class="pt-3 px-5 container-fluid" id="app">
    
    @*<partial name="_SideBarMenu" />*@
    <main role="main" class="pb-3">
        <!-- BEGIN PAGE BAR -->
        <div class="page-bar">
            <metronic-bread-crumb asp-homepage-title="الرئيسية"
                                  asp-homepage-url="@Url.Action("Index", "Home")"
                                  asp-homepage-glyphicon="fas fa-home">
            </metronic-bread-crumb>
        </div>
        <!-- END PAGE BAR -->
        <!-- BEGIN PAGE TITLE-->
        <h1 class="page-title">
            @RenderSection("title", false)
        </h1>
        <!-- END PAGE TITLE-->
        @RenderBody()
    </main>
</div>

<footer class="border-top footer text-muted">
    <div class="container">
        &copy; 2018 - ProjectTemplate.Web - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
    </div>
</footer>
<script src="~/js/app.js" asp-append-version="true"></script>


